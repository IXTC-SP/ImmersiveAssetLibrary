<div class="d-flex justify-content-between w-100">
    <nav class="nav flex-column uploads-nav">
      <a class="nav-link p-0 ps-3 uploads-assetActive" type="button" onclick="clickAssetType(this, 'uploads-')">3d Models</a>
      <a class="nav-link p-0 ps-3" type="button" onclick="clickAssetType(this, 'uploads-')">360 Images</a>
    </nav>
    <div class="row gap-3 uploads-row d-flex" id="uploads-3d Models">
      <%if(uploads.models.length>0){%>    
        <%uploads.models.forEach(card =>{%>
          <div class="card upload-card col-md-3 p-0">
            <div class="d-flex justify-content-end">
              <a href="#!" class="btn btn-outline-danger box-shadow-0 border-0"
                ><i class="fa-solid fa-trash-can delete-icon"></i></a
              >
            </div>
            <div class="upload-card-img">
              <img
                src= "<%= card.assetPath.folderpath.slice(1) +
                '/' +
                card.assetPath.thumbnail %>"
                class="card-img-top"
                alt="<%= card._id %>"
              />
            </div>
            <div class="card-body upload-card-body p-3">
              
                <h6 class="card-title m-0"><strong><%= card.title %></strong></h6>
              <!-- <% let tags = "" %>
              <% card.tags.forEach(tag =>{%>
                <% tags += tag %>              
              <%})%>
              <span class="card-text text-overflow"><small><%= tags %></small></span>        -->
              <span class="card-text text-overflow"><small><%= card.uploaddate.toLocaleString().split(",")[0] %></small></span>       

              
            </div>
          </div>
        <%})%>
      <%}else{%>
        <p class="text-center">You have not uploaded any 3d Models.</p>
      <%}%>
    </div>
    <div class="row gap-3 uploads-row d-none" id="uploads-360 Images">
      <%if(uploads["360"].length>0){%>   
        <%uploads["360"].forEach(card =>{%>
          <div class="card upload-card col-md-3 p-0">
            <div class="d-flex justify-content-end">
              <a href="#!" class="btn btn-outline-danger box-shadow-0 border-0"
                ><i class="fa-solid fa-trash-can delete-icon"></i></a
              >
            </div>
            <div class="upload-card-img">
              <img
                src= "<%= card.assetPath.folderpath.slice(1) +
                '/' +
                card.assetPath.thumbnail %>"
                class="card-img-top"
                alt="<%= card._id %>"
              />
            </div>
            <div class="card-body upload-card-body p-3">
              
                <h6 class="card-title m-0"><strong><%= card.title %></strong></h6>
              <!-- <% let tags = "" %>
              <% card.tags.forEach(tag =>{%>
                <% tags += tag %>              
              <%})%>
              <span class="card-text text-overflow"><small><%= tags %></small></span>        -->
              <span class="card-text text-overflow"><small><%= card.uploaddate.toLocaleString().split(",")[0] %></small></span>       
             
          
            
            </div>
          </div>
        <%})%>
      <%}else{%>
        <p class="text-center">You have not uploaded any 360 Images.</p>
      <%}%>
    </div>
</div>

